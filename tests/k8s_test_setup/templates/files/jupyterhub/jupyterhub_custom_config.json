{
  "drf-services": {
    "unicoremgr": {
      "authentication_token": "Not Set",
      "send_access_token": true,
      "request_timeout": 10,
      "certificate_path": "/home/jupyterhub/ca-root.pem",
      "urls": {
        "services": "https://unicoremgr-<ID>.<NAMESPACE>.svc/api/services/"
      }
    },
    "k8smgrhdfcloud": {
      "authentication_token": "Not Set",
      "request_timeout": 10,
      "certificate_path": "/home/jupyterhub/ca-root.pem",
      "urls": {
        "services": "https://k8smgr-<ID>.<NAMESPACE>.svc/api/services/"
      }
    },
    "tunnel": {
      "authentication_token": "Not Set",
      "request_timeout": 10,
      "certificate_path": "/home/jupyterhub/ca-root.pem",
      "urls": {
        "services": "https://tunnel-<ID>.<NAMESPACE>.svc/api/tunnel/"
      }
    }
  },
  "unity": {
    "revoke": {
      "url": "https://<UNITY_ALT_NAME>/oauth2/revoke",
      "certificate_path": false,
      "request_timeout": 10,
      "client_id": "oauth-client",
      "expected_status_code": 200
    }
  },
  "vos": {
    "default": {
      "authenticators": [
        "ldap"
      ],
      "usernames": [],
      "admin": false,
      "weight": 1,
      "Systems": {},
      "Services": {
        "JupyterLab": {
          "JupyterLab": {
            "max_per_user": 5
          }
        }
      },
      "description": "Default group"
    },
    "admin": {
      "authenticators": [],
      "usernames": [
        "jhub-admin@fz-juelich.de"
      ],
      "admin": true,
      "weight": 2,
      "Systems": {},
      "Services": {
        "JupyterLab": {
          "JupyterLab": {
            "max_per_user": 5
          }
        }
      },
      "description": "Admin group"
    }
  },
  "services": {
    "JupyterLab": {
      "options_name": "Version",
      "options": {
        "JupyterLab": {
          "name": "K8s Test JupyterLab",
          "vo_mapping": {
            "admins": "default"
          },
          "allowed_lists": {
            "systems": [
              "DEMO-SITE",
              "HDF-Cloud"
            ]
          }
        }
      }
    }
  },
  "systems": {
    "DEMO-SITE": {
      "base_url": "https://unicore-<ID>.<NAMESPACE>.svc/DEMO-SITE/rest/core",
      "interactive_partitions": [
        "LoginNode"
      ],
      "LoginNode": [
        "localhost"
      ],
      "nodes": [],
      "nodes_mapping": {},
      "fuser": "ljupyter",
      "host": "",
      "weight": 1,
      "drf-service": "unicoremgr"
    },
    "HDF-Cloud": {
      "drf-service": "k8smgrhdfcloud"
    }
  },
  "resources": {
    "DEMO-SITE": {
      "LoginNode": {},
      "batch": {
        "nodes": {
          "default": 1,
          "divisor": 1,
          "minmax": [
            1,
            10
          ]
        },
        "runtime": {
          "default": 30,
          "divisor": 60,
          "minmax": [
            10,
            1440
          ]
        }
      }
    }
  },
