{
  "services": {
    "base": "/tmp/files/services",
    "yaml_filename": "service.yaml",
    "descriptions": "/tmp/files/services_descriptions",
    "deployment_main_name_prefix": "depl-",
    "container_main_name": "main",
    "replace_indicators": ["<",">"],
    "replace_drfid_keyword": "id",
    "replace_uniqueuserid_keyword": "unique_user_id",
    "replace_secretname_keyword": "secret_name",
    "replace_secretcertsname_keyword": "secret_certs_name",
    "replace_namespace_keyword": "namespace",
    "replace_quotavo_keyword": "quotavo",
    "env": {
        "JUPYTERHUB_API_URL": "http://hdfcloud-jupyterhub-forward-<ID>:56789/hub/api"
    },
    "ssl": {
        "enabled": true,
        "notebooks_ca_crt_path": "/home/k8smgr/certs/notebooks-ca.crt",
        "notebooks_ca_key_path": "/home/k8smgr/certs/notebooks-ca.key",
        "ca_root_path": "/home/k8smgr/certs/ca-root.pem",
        "cn_prefix": "k8smgrhdfcloud-"
    }
  },
  "userhomes": {
    "base": "/tmp/userhomes",
    "skel": "/tmp/files/userhome_template"
  },
  "tunnel": {
    "k8smgr_hostname": "k8smgr_hdfcloud",
    "restart_url": "http://jupyterhub-<ID>.<NAMESPACE>.svc/hub/api/restarttunnel",
    "credentials_env_name": "TUNNEL_BASIC",
    "credentials_basic64": "<TUNNEL_K8SMGR_BASIC>",
    "timeout": 10,
    "certificate_path": "/home/k8smgr/certs/ca-root.pem"
  }
}
